      <articleItem id="1072" key="d2e0182d-7ac7-49b9-b8ee-39f1205d661c" parentID="1071" level="3" creatorID="0" sortOrder="0" createDate="2016-09-05T11:00:01" updateDate="2016-09-16T11:08:18" nodeName="Software" urlName="software" path="-1,1070,1071,1072" isDoc="" nodeType="1061" creatorName="Jakob Bak" writerName="Jakob Bak" writerID="0" template="1060" nodeTypeAlias="articleItem">
